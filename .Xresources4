*color15:  #f0d4bc

! Black color that will not be affected by bold highlighting.
*.color66: #0c0c08
*color66:  #0c0c08

! Xclock colors.
XClock*foreground: #f0d4bc
XClock*background: #0c0c08
XClock*majorColor:  rgba:f0/d4/bc/ff
XClock*minorColor:  rgba:f0/d4/bc/ff
XClock*hourColor:   rgba:f0/d4/bc/ff
XClock*minuteColor: rgba:f0/d4/bc/ff
XClock*secondColor: rgba:f0/d4/bc/ff

! Set depth to make transparency work.
URxvt*depth: 32

!############
!##  rofi  ##
!############

!                  bg       border   separator
rofi.color-window: #0c0c08, #0c0c08, #0c0c08
!                  bg       fg       bg-alt   hl-bg    hl-fg
rofi.color-normal: #0c0c08, #f0d4bc, #0c0c08, #f0d4bc, #0c0c08
rofi.color-active: #0c0c08, #f0d4bc, #0c0c08, #f0d4bc, #0c0c08
rofi.color-urgent: #0c0c08, #f0d4bc, #0c0c08, #f0d4bc, #0c0c08

rofi.lines: 		  	5
rofi.font:				Iosevka Term 15
rofi.padding: 			5
rofi.fixed-num-lines: 	false

URxvt.geometry: 		72x18
URxvt*letterSpace: 	    0
URxvt.lineSpace: 		0
URxvt*internalBorder:   40
URxvt*externalBorder:   0
URxvt*depth:            32
URxvt*saveline:         2000
URxvt*iso14755:         false
URxvt*scrollBar:        false
URxvt*scrollBar_right:  false
URxvt.perl-ext-common:  default,clipboard,url-select,keyboard-select
URxvt.urlLauncher:      firefox
URxvt.underlineURLs:    true
URxvt.urlButton:        1
URxvt*buffered:         false          
URxvt.urgentOnBell: 	True
URxvt*keysym.C-Shift-q: command:exit
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

! ------------------------------------------------------------------------------
! Xft Font Configuration
! ------------------------------------------------------------------------------
URxvt.font: xft:Iosevka Term:size=11
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
! Xft.rgba: rgb
