! Xclock colors.
XClock*foreground: #9bacbd
XClock*background: #0F0C14
XClock*majorColor:  rgba:9b/ac/bd/ff
XClock*minorColor:  rgba:9b/ac/bd/ff
XClock*hourColor:   rgba:9b/ac/bd/ff
XClock*minuteColor: rgba:9b/ac/bd/ff
XClock*secondColor: rgba:9b/ac/bd/ff

! Set depth to make transparency work.
URxvt*depth: 32

!############
!##  rofi  ##
!############

!                  bg       border   separator
rofi.color-window: #0F0C14, #0F0C14, #0F0C14
!                  bg       fg       bg-alt   hl-bg    hl-fg
rofi.color-normal: #0F0C14, #9bacbd, #0F0C14, #9bacbd, #0F0C14
rofi.color-active: #0F0C14, #9bacbd, #0F0C14, #9bacbd, #0F0C14
rofi.color-urgent: #0F0C14, #9bacbd, #0F0C14, #9bacbd, #0F0C14

rofi.lines: 		  	5
rofi.font:				SFNS Display 15
rofi.padding: 			5
rofi.fixed-num-lines: 	false

URxvt.geometry: 		72x18
URxvt*letterSpace: 	    0
URxvt.lineSpace: 		0
URxvt*internalBorder:   40
URxvt*externalBorder:   0
URxvt*depth:            32
URxvt*saveline:         2000
URxvt*iso14755:         false
URxvt*scrollBar:        false
URxvt*scrollBar_right:  false
URxvt.perl-ext-common:  default,clipboard,url-select,keyboard-select
URxvt.urlLauncher:      firefox
URxvt.underlineURLs:    true
URxvt.urlButton:        1
URxvt*buffered:         false          
URxvt.urgentOnBell: 	True
URxvt*keysym.C-Shift-q: command:exit
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

! ------------------------------------------------------------------------------
! Xft Font Configuration
! ------------------------------------------------------------------------------
URxvt.font: xft:Iosevka Term:size=11
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
