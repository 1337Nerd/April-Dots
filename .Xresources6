! Xclock colors.
XClock*foreground: #9ab7e4
XClock*background: #2d2e2a
XClock*majorColor:  rgba:9a/b7/e4/ff
XClock*minorColor:  rgba:9a/b7/e4/ff
XClock*hourColor:   rgba:9a/b7/e4/ff
XClock*minuteColor: rgba:9a/b7/e4/ff
XClock*secondColor: rgba:9a/b7/e4/ff

! Set depth to make transparency work.
URxvt*depth: 32

!############
!##  rofi  ##
!############

!                  bg       border   separator
rofi.color-window: #2d2e2a, #2d2e2a, #2d2e2a
!                  bg       fg       bg-alt   hl-bg    hl-fg
rofi.color-normal: #2d2e2a, #9ab7e4, #2d2e2a, #9ab7e4, #2d2e2a
rofi.color-active: #2d2e2a, #9ab7e4, #2d2e2a, #9ab7e4, #2d2e2a
rofi.color-urgent: #2d2e2a, #9ab7e4, #2d2e2a, #9ab7e4, #2d2e2a

rofi.lines: 		  	5
rofi.font:				Iosevka Term 15
rofi.padding: 			5
rofi.fixed-num-lines: 	false

URxvt.geometry: 		72x18
URxvt*letterSpace: 	    0
URxvt.lineSpace: 		0
URxvt*internalBorder:   40
URxvt*externalBorder:   0
URxvt*depth:            32
URxvt*saveline:         2000
URxvt*iso14755:         false
URxvt*scrollBar:        false
URxvt*scrollBar_right:  false
URxvt.perl-ext-common:  default,clipboard,url-select,keyboard-select
URxvt.urlLauncher:      firefox
URxvt.underlineURLs:    true
URxvt.urlButton:        1
URxvt*buffered:         false          
URxvt.urgentOnBell: 	True
URxvt*keysym.C-Shift-q: command:exit
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

! ------------------------------------------------------------------------------
! Xft Font Configuration
! ------------------------------------------------------------------------------
URxvt.font: xft:Iosevka Term:size=11
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
! Xft.rgba: rgb
